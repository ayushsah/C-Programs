#include<stdio.h>

main()
{
 int arr[5], rev[5], i,j;

 for(i=0;i<5;i++)
 {
  printf("Enter a number to store at arr[%d]\t",i);
  scanf("%d",&arr[i]);
 }
 for(i=0,j=4;i<5;i++,j--)
 {
  rev[j] = arr[i];
 }
 printf("\n\n");
 for(i=0;i<5;i++)
 {
  printf("Element before reversing at arr[%d] is %d\n",i,arr[i]);
 }
 printf("\n\n");
 for(j=0;j<5;j++)
 {
  printf("Element after reversing into the rev[%d] is %d\n",j,rev[j]);
 }
}
